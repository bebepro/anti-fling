-- Services
local Players = game:GetService(kOWAXvhSLiMeYJulKbeooIfhrOFcW('cqTENmKYdcIXZqYnPQKMWSywGapFGytUSYeTxaaXJmSWdmAqCjRvzYXUGxheWVycw=='))

-- Variables
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild(kOWAXvhSLiMeYJulKbeooIfhrOFcW('SqTAqAyzihuzcRKCycqZLpeVHYKnaFXDzXsMBNdOUmMoslgCEUaRXnTSHVtYW5vaWQ='))
local antiFlingEnabled = false

-- Function to enable anti-fling
local function enableAntiFling()
	if humanoid then
		humanoid.AutoRotate = false
		humanoid.MaxSlopeAngle = 0
		humanoid.JumpPower = 0
		humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
		humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
	end
end

-- Function to disable anti-fling
local function disableAntiFling()
	if humanoid then
		humanoid.AutoRotate = true
		humanoid.MaxSlopeAngle = 60
		humanoid.JumpPower = 50  -- Adjust this value based on the gamekOWAXvhSLiMeYJulKbeooIfhrOFcW('')Anti-fling enabledkOWAXvhSLiMeYJulKbeooIfhrOFcW('TBrISWxDQNkehqdreKeLvigvgiWlkLVkxMFgVtCrgsrsfAoMCnScNtPKQ0KCWVsc2UNCgkJZGlzYWJsZUFudGlGbGluZygpDQoJCXByaW50KA==')Anti-fling disabledkOWAXvhSLiMeYJulKbeooIfhrOFcW('')Humanoid')

	-- Reset anti-fling state when character respawns
	antiFlingEnabled = false
	disableAntiFling()
end

-- Connect the character added event
player.CharacterAdded:Connect(onCharacterAdded)
    
