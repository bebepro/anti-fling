data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild(gtjHCfRXoayqgZIiQLfzFOZwUtKOPEWfZTzOqRIjeUiUiBGYpowJuM('vkunDlMGGxxYPCxMhMzmZkVANXLMAuGmoXEpFJLostIrnDbDxfDwdOqSHVtYW5vaWQ='))
local isAntiFlingEnabled = false  -- Initial state for anti-fling
local connection  -- Variable to hold the connection for the heartbeat function

-- Function to prevent flinging
local function preventFling()
	if humanoid and humanoid.RootPart then
		humanoid.RootPart.Velocity = Vector3.new(0, humanoid.RootPart.Velocity.Y, 0)  -- Keep Y velocity to prevent fling
	end
end

-- Toggle anti-fling functionality
local function toggleAntiFling()
	isAntiFlingEnabled = not isAntiFlingEnabled  -- Toggle state
	if isAntiFlingEnabled then
		print(gtjHCfRXoayqgZIiQLfzFOZwUtKOPEWfZTzOqRIjeUiUiBGYpowJuM('nJstZjrYbYIhyAcHocUlPdvalOxNvsycfUyoBtzwkkmeyXwhnHFWQyzQW50aS1mbGluZyBFTkFCTEVE'))
		connection = game:GetService(gtjHCfRXoayqgZIiQLfzFOZwUtKOPEWfZTzOqRIjeUiUiBGYpowJuM('gnNivBGUXjkowUrocHuBVolPXVjyZoNtlOkpifyZJISOasszgkKkDwqUnVuU2VydmljZQ==')).Heartbeat:Connect(preventFling)  -- Connect the preventFling function
	else
		print(gtjHCfRXoayqgZIiQLfzFOZwUtKOPEWfZTzOqRIjeUiUiBGYpowJuM('mazoiZxZSPfTgFARYDjBkKRmVJBHKAfyMxsJwRmwGRuJrMaoFlWiidKQW50aS1mbGluZyBESVNBQkxFRA=='))
		if connection then
			connection:Disconnect()  -- Disconnect the connection to stop preventing fling
			connection = nil  -- Reset the connection variable
		end
	end
end

-- Connect the button click to the toggle function
script.Parent.MouseButton1Click:Connect(toggleAntiFling)
    
